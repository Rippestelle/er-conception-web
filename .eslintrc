{
  "extends": ["@nuxtjs/eslint-config-typescript"],
  "rules": {
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "semi": [
      "error",
      "always"
    ],
    "func-call-spacing": "warn",
    "vue/multi-word-component-names": "off",
    "vue/no-reserved-component-names": "off",
    "vue/no-multiple-template-root": "off",
    "vue/no-setup-props-destructure": "off",
    "vue/no-v-html": "off"
  }
}